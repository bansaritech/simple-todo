{
    "name": "Todo",
    "version": "1.0.0",
    "manifest_version": 3,
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "short_name": "Todo",
    "start_url": "/simple-todo/",
    "display": "standalone",
    "background_color": "#ffffff",
    "theme_color": "#000000",
    "icons": [
      {
        "src": "icon.png",
        "sizes": "192x192",
        "type": "image/png"
      },
      {
        "src": "icon.png",
        "sizes": "512x512",
        "type": "image/png"
      }
    ]
  }
  